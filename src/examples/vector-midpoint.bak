#lang rosette

(require "../ast.rkt"
         "../dsp-insts.rkt"
         "../interp.rkt"
         "../utils.rkt"
         "../prog-sketch.rkt"
         "../synth.rkt"
         "../configuration.rkt"
         racket/trace
         racket/generator
         rosette/lib/angelic)



(define (vector-midpoint v1 v2)
  (assert (= (length v1) (length v2))
          "VECTOR-MIDPOINT: length of vectors not equal")
  (for/list ([e1 v1]
             [e2 v2])
    (box (bvsdiv (bvadd (unbox e1) (unbox e2)) (bv-value 2)))))
